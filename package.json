    "axios": "^1.7.7",
    "bn.js": "^5.2.1",
    "express": "^4.21.2",
    "js-base64": "^3.7.7",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.10.0",
    "ollama": "^0.5.12",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "styled-components": "^6.1.13",
