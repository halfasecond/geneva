{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "resolveJsonModule": true,
    "paths": {
      "api-ts/*": ["./api-ts/*"]
    }
  },
  "include": [
    "vite.config.ts",
    "src/server/**/*",
    "api-ts/**/*"
  ]
}
